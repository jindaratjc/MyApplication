<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context="com.project.app.myapplication.MainActivity">



<Button
    android:id="@+id/button3"
    android:layout_width="wrap_content"
    android:layout_height="100dp"
    android:layout_above="@+id/textView"
    android:layout_alignParentStart="true"
    android:layout_margin="10dp"
    android:padding="20dip"
    android:background="@drawable/books_icon"
    android:text="" />

<Button
    android:id="@+id/button"
    android:layout_width="100dp"
    android:layout_height="100dp"
    android:layout_above="@+id/textView2"
    android:layout_centerHorizontal="true"
    android:layout_margin="10dp"
    android:padding="20dip"
    android:background="@drawable/food_icon"
    android:text="" />


<Button
    android:id="@+id/button2"
    android:layout_width="wrap_content"
    android:layout_height="100dp"
    android:layout_alignParentEnd="true"
    android:layout_alignTop="@+id/button"
    android:layout_margin="10dp"
    android:background="@drawable/phone_icon"
    android:padding="20dip"
    android:text="" />


<Button
    android:id="@+id/button4"
    android:layout_width="100dp"
    android:layout_height="100dp"
    android:layout_above="@+id/textView5"
    android:layout_alignStart="@+id/button"
    android:layout_marginBottom="14dp"
    android:background="@drawable/toilet_icon"
    android:text="" />

<Button
    android:id="@+id/button5"
    android:layout_width="100dp"
    android:layout_height="100dp"
    android:layout_above="@+id/textView5"
    android:layout_alignParentStart="true"
    android:layout_marginBottom="31dp"
    android:layout_marginLeft="10dp"
    android:layout_marginTop="10dp"
    android:background="@drawable/clothing_icon"
    android:text="" />

<Button
    android:id="@+id/button6"
    android:layout_width="100dp"
    android:layout_height="100dp"
    android:layout_alignStart="@+id/button2"
    android:layout_alignTop="@+id/button4"
    android:layout_marginStart="6dp"
    android:layout_marginTop="6dp"
    android:background="@drawable/atm_icon"
    android:text="" />

<Button
    android:id="@+id/button7"
    android:layout_width="100dp"
    android:layout_height="100dp"
    android:layout_alignParentBottom="true"
    android:layout_alignParentStart="true"
    android:layout_marginBottom="46dp"
    android:text="" />

<Button
    android:id="@+id/button8"
    android:layout_width="100dp"
    android:layout_height="100dp"
    android:layout_alignEnd="@+id/button4"
    android:layout_alignTop="@+id/button7"
    android:text="" />

<Button
    android:id="@+id/button9"
    android:layout_width="100dp"
    android:layout_height="100dp"
    android:layout_alignBottom="@+id/button8"
    android:layout_alignStart="@+id/button6"
    android:text="" />

<TextView
    android:id="@+id/textView"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_above="@+id/button5"
    android:layout_alignParentStart="true"
    android:layout_marginStart="16dp"
    android:text="ร้านหนังสือ" />

<TextView
    android:id="@+id/textView2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/button3"
    android:layout_centerHorizontal="true"
    android:text="ร้านอาหาร" />

<TextView
    android:id="@+id/textView3"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignStart="@+id/button2"
    android:layout_below="@+id/button2"
    android:layout_marginStart="15dp"
    android:layout_marginTop="6dp"
    android:text="ศูนย์บริการ" />

<TextView
    android:id="@+id/textView5"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_above="@+id/button7"
    android:layout_alignStart="@+id/textView"
    android:layout_marginBottom="43dp"
    android:text="ร้านเสื้อผ้า" />

<TextView
    android:id="@+id/textView6"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/button4"
    android:layout_centerHorizontal="true"
    android:text="ห้องน้ำ" />

<TextView
    android:id="@+id/textView7"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/button6"
    android:layout_marginStart="12dp"
    android:layout_toEndOf="@+id/editText"
    android:text="ATM"
    android:textAlignment="center" />

<EditText
    android:id="@+id/editText"
    android:layout_width="300dp"
    android:layout_height="50dp"
    android:layout_alignParentTop="true"
    android:layout_marginTop="14dp"
    android:layout_toStartOf="@+id/button10"
    android:ems="10"
    android:hint="ค้นหาสถานที่"
    android:inputType="textPersonName"
    android:text=""
    android:textAlignment="center" />

<Button
    android:id="@+id/button10"
    android:layout_width="wrap_content"
    android:layout_height="50dp"
    android:layout_alignParentEnd="true"
    android:layout_alignTop="@+id/editText"
    android:text="ค้นหา" />

</RelativeLayout>
